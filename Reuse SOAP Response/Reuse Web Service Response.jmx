        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Create Contry By Code Request" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import groovy.xml.*
def soapResponseBody = new XmlSlurper().parseText(vars.get(&apos;soapResponseBody&apos;))
def country = soapResponseBody.ListOfCountryNamesByCodeResponse.ListOfCountryNamesByCodeResult.tCountryCodeAndName[0];

def builder = new StreamingMarkupBuilder()
builder.encoding = &apos;UTF-8&apos;
def soapRequest = builder.bind {
    mkp.xmlDeclaration()
    namespaces &lt;&lt; [web:&apos;http://localhost/countryinfo&apos;,soapenv:&apos;http://www.w3.org/2003/05/soap-envelope&apos;]  
    soapenv.Envelope() {
		soapenv.Body(){
			web.CountryName(){
				web.sCountryISOCode country.sISOCode.text()									
			}
		}
	}
}
 
vars.put(&quot;soapRequest&quot;, XmlUtil.serialize(soapRequest))
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
